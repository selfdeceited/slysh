{
  "manifest_version": 2,
  "name": "Slysh",
  "version": "0.1.0",
  "description": "(self) time management for dummies",
  "icons": {
    "128": "favicon.jpg"
  },
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "permissions": ["https://*.google.com/", "activeTab"],
  "browser_action": {
    "default_icon": "favicon.jpg",
    "default_popup": "index.html"
  }
}
